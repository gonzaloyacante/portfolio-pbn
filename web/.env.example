# Web Environment Variables Example
# Copy this file to .env and fill with your actual values
# All variables prefixed with NEXT_PUBLIC_ are exposed to the browser

# Supabase (Public)
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"

# Cloudinary (Public)
NEXT_PUBLIC_CLOUDINARY_API_KEY="your-cloudinary-key"
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME="your-cloud-name"

# API URL
NEXT_PUBLIC_API_URL="http://localhost:5000/api"

# App
NEXT_PUBLIC_APP_NAME="portfolio-pbn"
