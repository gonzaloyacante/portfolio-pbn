{
  "name": "portfolio-pbn",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true prisma generate && ANALYZE=true next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "rm -rf .next node_modules",
    "fresh": "pnpm run clean && pnpm install && npx prisma generate",
    "type-check": "npx tsc --noEmit",
    "check:all": "pnpm run type-check && pnpm run lint",
    "verify": "pnpm run check:all && pnpm run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "validate:env": "npx tsx scripts/validate-env.ts",
    "db:migrate": "npx tsx scripts/migrate.ts",
    "pre-commit": "npx tsx scripts/pre-commit-checks.ts",
    "stress:seed": "npx tsx scripts/seed-stress-test.ts",
    "stress:clean": "npx tsx scripts/delete-test-data.ts",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "prisma db seed",
    "db:seed:admin:dev": "npx tsx scripts/seed-admin-dev.ts",
    "db:seed:admin:prod": "npx tsx scripts/seed-admin-prod.ts",
    "db:fresh": "pnpm db:reset && pnpm db:seed",
    "db:push": "prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:studio": "prisma studio",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "7.4.1",
    "@sentry/nextjs": "^10.39.0",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.3.0",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.34.3",
    "isomorphic-dompurify": "^2.36.0",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "nprogress": "^0.2.0",
    "pg": "^8.18.0",
    "prisma": "7.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.2",
    "react-hot-toast": "^2.6.0",
    "react-simple-maps": "^3.0.0",
    "resend": "^6.9.2",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-scale": "^4.0.9",
    "@types/pg": "^8.16.0",
    "@types/react-simple-maps": "^3.0.6",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.3.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.3",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "d3-color": "^3.1.0",
      "hono": "^4.11.7",
      "minimatch": "^10.2.1",
      "lodash": "^4.17.21"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@sentry/cli",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.28.0"
}
